        <script>
            $(document).ready(function() {
                $("#magnifier").click(function(){
                    if( $("#inputval").val() == 0 ){
                        $("#inputval").val("1");
                        var oldSize = parseFloat($("#content").css('font-size'));
                        var newSize = oldSize  * 2;
                        $(".content").hover(
                            function() {
                                $("#content").animate({ fontSize: newSize}, 200);
                            },
                            function() {
                                $("#content").animate({ fontSize: oldSize}, 200);
                            }
                        );
                    } else {
                        $("#inputval").val("0");
                        location.reload();
                    }
                })
            });
        </script>
