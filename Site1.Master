            var msg = "हि लिंक नविन विंडोमध्ये उघडेल आणि तुम्हाला वेगळ्या साईटवर घेऊन जाईल.पुढे जाण्यासाठी OK क्लिक करा.";
        }
        var r = confirm( msg );
        if( r == true ) {
            window.open(url, '_blank' );
        }
    }

